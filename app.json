{
  "expo": {
    "name": "Afood",
    "slug": "Afood",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logo.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "scheme": "Afood",
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "buildNumber": "6",
      "bundleIdentifier": "com.Afoudusers.app",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "NSLocationWhenInUseUsageDescription": "We need your location to show you nearby delivery locations",
        "NSLocationAlwaysAndWhenInUseUsageDescription": "We need your location to show you nearby delivery locations",
        "NSCameraUsageDescription": "This app needs access to camera to take photos of food items and upload images"
      },
      "config": {
        "googleMapsApiKey": "AIzaSyA-sjBct3sZ45E4wGLg4rVmboTuH5UgQHQ"
      }
    },
    "android": {
      "versionCode": 5,
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.Afoodusers.app",
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyBh59DQJEXejCeQSWZM083i_x-1Fav_GXs"
        }
      },
      "permissions": [
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION",
        "com.google.android.gms.permission.AD_ID"
      ]
    },
    "plugins": [
      "expo-router",
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow Food Delivery to use your location."
        }
      ],
      [
        "@stripe/stripe-react-native",
        {
          "enableGooglePay": false
        }
      ],
      [
        "expo-build-properties",
        {
          "android": {
            "compileSdkVersion": 35,
            "targetSdkVersion": 34,
            "buildToolsVersion": "34.0.0"
          },
          "ios": {
            "deploymentTarget": "15.1"
          }
        }
      ],
      "expo-tracking-transparency"
    ],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "97d09126-0e35-4d00-aa38-3b4ab5b7edc0"
      }
    }
  }
}
